                    function startsWith(str, maybePrefix) {
                        //https://stackoverflow.com/a/4579228
                        return str.lastIndexOf(maybePrefix, 0) === 0
                    }

                    var onArrowClick = function(company_line) {
                        target = company_line.querySelector('.arrow');
                        if (target.className.includes('open')){
                          target.className = 'arrow';  
                          company_line.parentNode.parentNode.querySelector('.accordion-item__description').style.display='block';
                        } else{
                          target.className = 'arrow open';
                          company_line.parentNode.parentNode.querySelector('.accordion-item__description').style.display='none';
                        }
                    };


                    var onSectorClick = function(sector_line) {
                        //target = sector_line.querySelector('.arrow');
                        var companiesInSector = sector_line.parentNode.querySelector('.accordion-block');
                        if (companiesInSector.style.display == 'block'){
                          sector_line.parentNode.querySelector('.accordion-block').style.display='none';
                        } else {
                          sector_line.parentNode.querySelector('.accordion-block').style.display='block';
                        }
                        /*
                        if (target.className.includes('open')){
                          target.className = 'arrow';  
                          sector_line.parentNode.querySelector('.accordion-block').style.display='none';
                        } else {
                          target.className = 'arrow open';
                          sector_line.parentNode.querySelector('.accordion-block').style.display='block';
                        }
                        */
                    };

